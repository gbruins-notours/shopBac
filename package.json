  "scripts": {
    "after-deploy": "npm install && npm run postinstall && npm run build && npm run knex:migrate",
    "dev": "cross-env NODE_ENV=development nodemon server/start.js --watch server",
    "build": "nuxt build",
    "gensecret": "node ./utils/gensecret.js",
    "knex:migrate": "node ./node_modules/knex/bin/cli.js migrate:latest --knexfile ./knexfile.js",
    "knex:seed": "node --require dotenv/config ./node_modules/knex/bin/cli.js seed:run --knexfile ./knexfile.js",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "test:server": "NODE_ENV=test lab -a @hapi/code -v --leaks test/server",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
